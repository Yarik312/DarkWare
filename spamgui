local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
	Title = "DarkWare",
	Style = 1,
	SizeX = 300,
	SizeY = 200,
	Theme = "Dark",
	ColorOverrides = {
		MainFrame = Color3.fromRGB(38, 34, 34)
	}
})

local K = X.New({
    Title = "SPAM"
})

local A = K.Button({
	Text = "turn on spam",
	Callback = function()
        _G.Spam = true
        while _G.Spam do
        wait(0.1)
        local args = {
            [1] = "get good, get DARKWARE.TK, get good, get DARKWARE.TK, get good, get DARKWARE.TK, get good, get DARKWARE.TK,get good, get DARKWARE.TK, get good, get DARKWARE.TK",
            [2] = "All"
        }
        
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
        end
	end,
	Menu = {
		Information = function(self)
			X.Banner({
				Text = "on spam"
			})
		end
	}
})


local A = K.Button({
	Text = "turn off spam",
	Callback = function()
        _G.Spam = false
        while _G.Spam do
        wait(0.1)
        local args = {
            [1] = "get good, get DARKWARE.TK, get good, get DARKWARE.TK, get good, get DARKWARE.TK, get good, get DARKWARE.TK,get good, get DARKWARE.TK, get good, get DARKWARE.TK",
            [2] = "All"
        }
        
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
        end
	end,
	Menu = {
		Information = function(self)
			X.Banner({
				Text = "off spam"
			})
		end
	}
})
